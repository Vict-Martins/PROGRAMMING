#include <stdio.h>
#include <stdlib.h>



main(){
    int x,y, C = 0;
    char T[2];
    double soma = 0, M[12][12];


    scanf("%d",&C);
    scanf("%s",&T);

    for(x = 0;x <= 11;x++){
        for(y = 0;y <= 11;y++){

            scanf("%lf",&M[x][y]);

            if(x == C)
                soma+=M[x][y];

        }
    }

    if(T[0] =='S')
        printf("%.1lf\n",soma);
    else if(T[0] == 'M')
        soma = soma/12.0;
        printf("%.1lf\n",soma);

    return 0;
}
