    #include <stdio.h>
    #include <math.h>


    int main(){

        int A,B,AUX,MAIOR,MENOR,HORA;
        scanf("%d %d",&A,&B);

        AUX = 24;

if (A>=B){
        if(A>B){
            MAIOR = A;
            MENOR = B;
        }
        else{
            MAIOR = B;
            MENOR = A;
        }
        HORA = AUX-(MAIOR-MENOR);
        printf("O JOGO DUROU %d HORA(S)\n",HORA);
}
else{
        HORA = B-A;
        printf("O JOGO DUROU %d HORA(S)\n",HORA);
}
      return 0;
    }


