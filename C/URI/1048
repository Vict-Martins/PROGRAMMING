    #include <stdio.h>
    #include <math.h>


int main(){

    float s,n;
    scanf("%f",&s);


    if(s>=0 && s<=400){
        n = s*1.15;
        printf("Novo salario: %.2f\nReajuste ganho: %.2f\nEm percentual: 15 %%\n",n,n-s);
    }
    else if(s>400 && s<=800){
        n = s*1.12;
        printf("Novo salario: %.2f\nReajuste ganho: %.2f\nEm percentual: 12 %%\n",n,n-s);
    }
    else if(s>800 && s<=1200){
        n = s*1.10;
        printf("Novo salario: %.2f\nReajuste ganho: %.2f\nEm percentual: 10 %%\n",n,n-s);
    }
    else if(s>1200 && s<=2000){
        n = s*1.07;
        printf("Novo salario: %.2f\nReajuste ganho: %.2f\nEm percentual: 7 %%\n",n,n-s);
    }
    else if(s>2000){
        n = s*1.04;
        printf("Novo salario: %.2f\nReajuste ganho: %.2f\nEm percentual: 4 %%\n",n,n-s);
    }
      return 0;
    }


