#include <stdio.h>
#define MAX 99999
#include <stdlib.h>


int main(){


int i = 0,q = 0, a[MAX] ,x = 0, aux = 0;

while(1){

    scanf("%d",&a[i]);

    if(a[i] == 0)
        break;

i++;

}

aux = a[0];

for(i = 1 ; i <= aux ; i++ ){

    printf("%d",i);

    if((i == aux) == 0)
        printf(" ");

    if(i == aux){
        i = 0;
        x++;
        printf("\n");
        aux = a[x];
    }
}

return 0;
}
